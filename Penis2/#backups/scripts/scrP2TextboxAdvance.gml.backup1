/// scrP2TextboxAdvance
#args inst
with (inst) {
    if (!scrP2TextboxTypewriterFinished(inst)) {
        text_char_index = string_length(text_cut[|text_index]);
        event_user(1);
    } else if (text_index + 1 < ds_list_size(text_cut)) {
        text_index++;
        text_char_index = 0;
    } else {
        instance_destroy();
    }
}
