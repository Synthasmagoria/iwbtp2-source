/// scrP2WorldCreateForestTile(x, y, top)
#args xx, yy, top
with (oP2World) {
    var _tile = forest_tile_pool[|forest_tile_pool_index];
    _tile[?"x"] = xx;
    _tile[?"y"] = yy;
    _tile[?"top"] = top;
    ds_list_add(forest_tiles, _tile);
    forest_tile_pool_index = (forest_tile_pool_index + 1) % ds_list_size(forest_tile_pool);
}